<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
  xmlns:r='urn:import:medizin.client.scaffold.ui'
  xmlns:a='urn:import:medizin.client.managed.ui'>
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div ui:field='errors' class='{style.error}'></div>
    
    <div id='name'><div class='{style.label}'>Name</div><div class='{style.value}'><g:TextBox ui:field='name'/></div></div>
    <div id='dateOfAssesment'><div class='{style.label}'>Date Of Assesment</div><div class='{style.value}'><d:DateBox ui:field='dateOfAssesment'/></div></div>
    <div id='dateOpen'><div class='{style.label}'>Date Open</div><div class='{style.value}'><d:DateBox ui:field='dateOpen'/></div></div>
    <div id='dateClosed'><div class='{style.label}'>Date Closed</div><div class='{style.value}'><d:DateBox ui:field='dateClosed'/></div></div>
    <div id='place'><div class='{style.label}'>Place</div><div class='{style.value}'><g:TextBox ui:field='place'/></div></div>
    <div id='logo'><div class='{style.label}'>Logo</div><div class='{style.value}'><g:TextBox ui:field='logo'/></div></div>
    <div id='isClosed'><div class='{style.label}'>Is Closed</div><div class='{style.value}'><g:CheckBox ui:field='isClosed'/></div></div>
    <div id='assesmentVersion'><div class='{style.label}'>Assesment Version</div><div class='{style.value}'><g:IntegerBox ui:field='assesmentVersion'/></div></div>
    <div id='mc'><div class='{style.label}'>Mc</div><div class='{style.value}'><g:ValueListBox ui:field='mc'/></div></div>
    <div id='repeFor'><div class='{style.label}'>Repe For</div><div class='{style.value}'><g:ValueListBox ui:field='repeFor'/></div></div>
    <div id='percentSameQuestion'><div class='{style.label}'>Percent Same Question</div><div class='{style.value}'><g:IntegerBox ui:field='percentSameQuestion'/></div></div>

    <g:Button ui:field='save'>Save</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
