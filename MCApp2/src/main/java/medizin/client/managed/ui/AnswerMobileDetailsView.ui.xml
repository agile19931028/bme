<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='dateAdded'><div class='{style.label}'>Date Added</div><div class='{style.value}' ui:field='dateAdded'></div></div>
    <div id='dateChanged'><div class='{style.label}'>Date Changed</div><div class='{style.value}' ui:field='dateChanged'></div></div>
    <div id='rewiewer'><div class='{style.label}'>Rewiewer</div><div class='{style.value}' ui:field='rewiewer'></div></div>
    <div id='autor'><div class='{style.label}'>Autor</div><div class='{style.value}' ui:field='autor'></div></div>
    <div id='answerText'><div class='{style.label}'>Answer Text</div><div class='{style.value}' ui:field='answerText'></div></div>
    <div id='isAnswerActive'><div class='{style.label}'>Is Answer Active</div><div class='{style.value}' ui:field='isAnswerActive'></div></div>
    <div id='isPicture'><div class='{style.label}'>Is Picture</div><div class='{style.value}' ui:field='isPicture'></div></div>
    <div id='isAnswerAcceptedReviewWahrer'><div class='{style.label}'>Is Answer Accepted Review Wahrer</div><div class='{style.value}' ui:field='isAnswerAcceptedReviewWahrer'></div></div>
    <div id='isAnswerAcceptedAutor'><div class='{style.label}'>Is Answer Accepted Autor</div><div class='{style.value}' ui:field='isAnswerAcceptedAutor'></div></div>
    <div id='isAnswerAcceptedAdmin'><div class='{style.label}'>Is Answer Accepted Admin</div><div class='{style.value}' ui:field='isAnswerAcceptedAdmin'></div></div>
    <div id='validity'><div class='{style.label}'>Validity</div><div class='{style.value}' ui:field='validity'></div></div>
    <div id='picturePath'><div class='{style.label}'>Picture Path</div><div class='{style.value}' ui:field='picturePath'></div></div>
    <div id='question'><div class='{style.label}'>Question</div><div class='{style.value}' ui:field='question'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
