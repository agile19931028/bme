<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class='{style.underline}'>
      <h3 class='{style.header}'><ui:msg><span ui:field='displayRenderer' /></ui:msg></h3>
    </div>

    <table id='boundElementHolder' class='{style.fields}'>
      <tr id='dateAdded'><td><span class='{style.label}'>Date Added:</span></td><td><span ui:field='dateAdded'></span></td></tr>
      <tr id='dateChanged'><td><span class='{style.label}'>Date Changed:</span></td><td><span ui:field='dateChanged'></span></td></tr>
      <tr id='rewiewer'><td><span class='{style.label}'>Rewiewer:</span></td><td><span ui:field='rewiewer'></span></td></tr>
      <tr id='autor'><td><span class='{style.label}'>Autor:</span></td><td><span ui:field='autor'></span></td></tr>
      <tr id='id'><td><span class='{style.label}'>Id:</span></td><td><span ui:field='id'></span></td></tr>
      <tr id='version'><td><span class='{style.label}'>Version:</span></td><td><span ui:field='version'></span></td></tr>
      <tr id='orderAversion'><td><span class='{style.label}'>Order Aversion:</span></td><td><span ui:field='orderAversion'></span></td></tr>
      <tr id='orderBversion'><td><span class='{style.label}'>Order Bversion:</span></td><td><span ui:field='orderBversion'></span></td></tr>
      <tr id='trenschaerfe'><td><span class='{style.label}'>Trenschaerfe:</span></td><td><span ui:field='trenschaerfe'></span></td></tr>
      <tr id='schwierigkeit'><td><span class='{style.label}'>Schwierigkeit:</span></td><td><span ui:field='schwierigkeit'></span></td></tr>
      <tr id='isAssQuestionAcceptedRewiever'><td><span class='{style.label}'>Is Ass Question Accepted Rewiever:</span></td><td><span ui:field='isAssQuestionAcceptedRewiever'></span></td></tr>
      <tr id='isAssQuestionAcceptedAdmin'><td><span class='{style.label}'>Is Ass Question Accepted Admin:</span></td><td><span ui:field='isAssQuestionAcceptedAdmin'></span></td></tr>
      <tr id='isAssQuestionAdminProposal'><td><span class='{style.label}'>Is Ass Question Admin Proposal:</span></td><td><span ui:field='isAssQuestionAdminProposal'></span></td></tr>
      <tr id='isAssQuestionAcceptedAutor'><td><span class='{style.label}'>Is Ass Question Accepted Autor:</span></td><td><span ui:field='isAssQuestionAcceptedAutor'></span></td></tr>
      <tr id='question'><td><span class='{style.label}'>Question:</span></td><td><span ui:field='question'></span></td></tr>
      <tr id='assesment'><td><span class='{style.label}'>Assesment:</span></td><td><span ui:field='assesment'></span></td></tr>
      <tr id='answersToAssQuestion'><td><span class='{style.label}'>Answers To Ass Question:</span></td><td><span ui:field='answersToAssQuestion'></span></td></tr>
    </table>

  <div class="{style.bar}">
    <g:Button addStyleNames='{style.button}' ui:field='edit'>Edit</g:Button>
    <g:Anchor ui:field='delete'>Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
