<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='responsiblePerson'><div class='{style.label}'>Responsible Person</div><div class='{style.value}' ui:field='responsiblePerson'></div></div>
    <div id='questionEvent'><div class='{style.label}'>Question Event</div><div class='{style.value}' ui:field='questionEvent'></div></div>
    <div id='assesment'><div class='{style.label}'>Assesment</div><div class='{style.value}' ui:field='assesment'></div></div>
    <div id='questionSum'><div class='{style.label}'>Question Sum</div><div class='{style.value}' ui:field='questionSum'></div></div>
    <div id='sort_order'><div class='{style.label}'>Sort_order</div><div class='{style.value}' ui:field='sort_order'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
