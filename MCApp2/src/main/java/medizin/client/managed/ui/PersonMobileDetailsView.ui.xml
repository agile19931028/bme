<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>
    <div id='name'><div class='{style.label}'>Name</div><div class='{style.value}' ui:field='name'></div></div>
    <div id='prename'><div class='{style.label}'>Prename</div><div class='{style.value}' ui:field='prename'></div></div>
    <div id='shidId'><div class='{style.label}'>Shid Id</div><div class='{style.value}' ui:field='shidId'></div></div>
    <div id='email'><div class='{style.label}'>Email</div><div class='{style.value}' ui:field='email'></div></div>
    <div id='alternativEmail'><div class='{style.label}'>Alternativ Email</div><div class='{style.value}' ui:field='alternativEmail'></div></div>
    <div id='phoneNumber'><div class='{style.label}'>Phone Number</div><div class='{style.value}' ui:field='phoneNumber'></div></div>
    <div id='isAdmin'><div class='{style.label}'>Is Admin</div><div class='{style.value}' ui:field='isAdmin'></div></div>
    <div id='isAccepted'><div class='{style.label}'>Is Accepted</div><div class='{style.value}' ui:field='isAccepted'></div></div>
    <div id='questionAccesses'><div class='{style.label}'>Question Accesses</div><div class='{style.value}' ui:field='questionAccesses'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
