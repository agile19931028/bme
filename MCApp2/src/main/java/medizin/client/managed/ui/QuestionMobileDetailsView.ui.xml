<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='questionText'><div class='{style.label}'>Question Text</div><div class='{style.value}' ui:field='questionText'></div></div>
    <div id='picturePath'><div class='{style.label}'>Picture Path</div><div class='{style.value}' ui:field='picturePath'></div></div>
    <div id='questionVersion'><div class='{style.label}'>Question Version</div><div class='{style.value}' ui:field='questionVersion'></div></div>
    <div id='isAcceptedRewiever'><div class='{style.label}'>Is Accepted Rewiever</div><div class='{style.value}' ui:field='isAcceptedRewiever'></div></div>
    <div id='isAcceptedAdmin'><div class='{style.label}'>Is Accepted Admin</div><div class='{style.value}' ui:field='isAcceptedAdmin'></div></div>
    <div id='isActive'><div class='{style.label}'>Is Active</div><div class='{style.value}' ui:field='isActive'></div></div>
    <div id='previousVersion'><div class='{style.label}'>Previous Version</div><div class='{style.value}' ui:field='previousVersion'></div></div>
    <div id='keywords'><div class='{style.label}'>Keywords</div><div class='{style.value}' ui:field='keywords'></div></div>
    <div id='questEvent'><div class='{style.label}'>Quest Event</div><div class='{style.value}' ui:field='questEvent'></div></div>
    <div id='comment'><div class='{style.label}'>Comment</div><div class='{style.value}' ui:field='comment'></div></div>
    <div id='questionType'><div class='{style.label}'>Question Type</div><div class='{style.value}' ui:field='questionType'></div></div>
    <div id='mcs'><div class='{style.label}'>Mcs</div><div class='{style.value}' ui:field='mcs'></div></div>
    <div id='answers'><div class='{style.label}'>Answers</div><div class='{style.value}' ui:field='answers'></div></div>
    <div id='dateAdded'><div class='{style.label}'>Date Added</div><div class='{style.value}' ui:field='dateAdded'></div></div>
    <div id='dateChanged'><div class='{style.label}'>Date Changed</div><div class='{style.value}' ui:field='dateChanged'></div></div>
    <div id='rewiewer'><div class='{style.label}'>Rewiewer</div><div class='{style.value}' ui:field='rewiewer'></div></div>
    <div id='autor'><div class='{style.label}'>Autor</div><div class='{style.value}' ui:field='autor'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
