<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='name'><div class='{style.label}'>Name</div><div class='{style.value}' ui:field='name'></div></div>
    <div id='dateOfAssesment'><div class='{style.label}'>Date Of Assesment</div><div class='{style.value}' ui:field='dateOfAssesment'></div></div>
    <div id='dateOpen'><div class='{style.label}'>Date Open</div><div class='{style.value}' ui:field='dateOpen'></div></div>
    <div id='dateClosed'><div class='{style.label}'>Date Closed</div><div class='{style.value}' ui:field='dateClosed'></div></div>
    <div id='place'><div class='{style.label}'>Place</div><div class='{style.value}' ui:field='place'></div></div>
    <div id='logo'><div class='{style.label}'>Logo</div><div class='{style.value}' ui:field='logo'></div></div>
    <div id='isClosed'><div class='{style.label}'>Is Closed</div><div class='{style.value}' ui:field='isClosed'></div></div>
    <div id='assesmentVersion'><div class='{style.label}'>Assesment Version</div><div class='{style.value}' ui:field='assesmentVersion'></div></div>
    <div id='mc'><div class='{style.label}'>Mc</div><div class='{style.value}' ui:field='mc'></div></div>
    <div id='repeFor'><div class='{style.label}'>Repe For</div><div class='{style.value}' ui:field='repeFor'></div></div>
    <div id='percentSameQuestion'><div class='{style.label}'>Percent Same Question</div><div class='{style.value}' ui:field='percentSameQuestion'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
