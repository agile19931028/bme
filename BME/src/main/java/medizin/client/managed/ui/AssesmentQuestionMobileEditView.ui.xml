<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
  xmlns:r='urn:import:medizin.client.scaffold.ui'
  xmlns:a='urn:import:medizin.client.managed.ui'>
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div ui:field='errors' class='{style.error}'></div>
    
    <div id='dateAdded'><div class='{style.label}'>Date Added</div><div class='{style.value}'><d:DateBox ui:field='dateAdded'/></div></div>
    <div id='dateChanged'><div class='{style.label}'>Date Changed</div><div class='{style.value}'><d:DateBox ui:field='dateChanged'/></div></div>
    <div id='rewiewer'><div class='{style.label}'>Rewiewer</div><div class='{style.value}'><g:ValueListBox ui:field='rewiewer'/></div></div>
    <div id='autor'><div class='{style.label}'>Autor</div><div class='{style.value}'><g:ValueListBox ui:field='autor'/></div></div>
    <div id='orderAversion'><div class='{style.label}'>Order Aversion</div><div class='{style.value}'><g:IntegerBox ui:field='orderAversion'/></div></div>
    <div id='orderBversion'><div class='{style.label}'>Order Bversion</div><div class='{style.value}'><g:IntegerBox ui:field='orderBversion'/></div></div>
    <div id='trenschaerfe'><div class='{style.label}'>Trenschaerfe</div><div class='{style.value}'><g:DoubleBox ui:field='trenschaerfe'/></div></div>
    <div id='schwierigkeit'><div class='{style.label}'>Schwierigkeit</div><div class='{style.value}'><g:DoubleBox ui:field='schwierigkeit'/></div></div>
    <div id='isAssQuestionAcceptedRewiever'><div class='{style.label}'>Is Ass Question Accepted Rewiever</div><div class='{style.value}'><g:CheckBox ui:field='isAssQuestionAcceptedRewiever'/></div></div>
    <div id='isAssQuestionAcceptedAdmin'><div class='{style.label}'>Is Ass Question Accepted Admin</div><div class='{style.value}'><g:CheckBox ui:field='isAssQuestionAcceptedAdmin'/></div></div>
    <div id='isAssQuestionAdminProposal'><div class='{style.label}'>Is Ass Question Admin Proposal</div><div class='{style.value}'><g:CheckBox ui:field='isAssQuestionAdminProposal'/></div></div>
    <div id='isAssQuestionAcceptedAutor'><div class='{style.label}'>Is Ass Question Accepted Autor</div><div class='{style.value}'><g:CheckBox ui:field='isAssQuestionAcceptedAutor'/></div></div>
    <div id='question'><div class='{style.label}'>Question</div><div class='{style.value}'><g:ValueListBox ui:field='question'/></div></div>
    <div id='assesment'><div class='{style.label}'>Assesment</div><div class='{style.value}'><g:ValueListBox ui:field='assesment'/></div></div>
    <div id='answersToAssQuestion'><div class='{style.label}'>Answers To Ass Question</div><div class='{style.value}'><a:AnswerToAssQuestionSetEditor ui:field='answersToAssQuestion'/></div></div>

    <g:Button ui:field='save'>Save</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
