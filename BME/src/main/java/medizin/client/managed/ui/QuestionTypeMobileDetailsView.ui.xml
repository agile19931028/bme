<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='questionTypeName'><div class='{style.label}'>Question Type Name</div><div class='{style.value}' ui:field='questionTypeName'></div></div>
    <div id='isWeil'><div class='{style.label}'>Is Weil</div><div class='{style.value}' ui:field='isWeil'></div></div>
    <div id='trueAnswers'><div class='{style.label}'>True Answers</div><div class='{style.value}' ui:field='trueAnswers'></div></div>
    <div id='falseAnswers'><div class='{style.label}'>False Answers</div><div class='{style.value}' ui:field='falseAnswers'></div></div>
    <div id='sumAnswers'><div class='{style.label}'>Sum Answers</div><div class='{style.value}' ui:field='sumAnswers'></div></div>
    <div id='maxLetters'><div class='{style.label}'>Max Letters</div><div class='{style.value}' ui:field='maxLetters'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
